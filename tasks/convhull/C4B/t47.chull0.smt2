(declare-const delta_n Int)
(declare-const delta___cost Int)
(declare-const delta_flag Int)
(assert (exists
         ((|n'?1| Int) (|flag'?2| Int) (|__cost'?3| Int) (phi_n?4 Int)
            (phi_flag?5 Int) (n?6 Int) (flag?7 Int) (__cost?8 Int))
         (and (<= (- flag?7) 0) (< (- flag?7) 0) (<= (- __cost?8) 0)
                (<= (+ (- __cost?8) -1) 0)
                (or (and (< (- n?6) 0) (= (+ (- phi_flag?5) 1) 0)
                           (= (+ (- phi_n?4) n?6 -1) 0))
                      (and (<= n?6 0) (= (- phi_flag?5) 0)
                             (= (+ (- phi_n?4) n?6) 0)))
                (= (+ |n'?1| (- phi_n?4)) 0)
                (= (+ |flag'?2| (- phi_flag?5)) 0)
                (= (+ |__cost'?3| (- __cost?8) -1) 0)
                (= delta___cost (+ |__cost'?3| (- __cost?8)))
                (= delta_flag (+ |flag'?2| (- flag?7)))
                (= delta_n (+ |n'?1| (- n?6))))))
(check-sat)
